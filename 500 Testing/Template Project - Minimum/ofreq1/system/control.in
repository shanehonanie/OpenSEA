seafile
{
    version     1.0;
    format      ascii;
    object      dictionary;
}


// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//The system object controls major settings for the run of oFreq.
system {
  //Analysis Type:  "response" or "resonance"
  analysis "response";

  //Wave frequencies [rad/s]
  frequency (
  0.001
  );

  //Wave directions [rad]
  //Angle of 0 = True North, proceeding counter-clockwise
  direction (
  0.0000
  );

  //Model of incident wave distribution to use
  wavemodel "wavemodel1"
}
